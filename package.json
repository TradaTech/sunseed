{
  "name": "@iceteachain/sunseed",
  "version": "0.2.0",
  "description": "Compile djs to raw js for deploying on Icetea Blockchain.",
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "install": "npm run build",
    "test": "jest --coverage",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TradaTech/sunseed.git"
  },
  "author": "Trada Tech <iceteachain@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TradaTech/sunseed/issues"
  },
  "homepage": "https://github.com/TradaTech/sunseed#readme",
  "dependencies": {
    "@babel/core": "7.12.8",
    "@babel/parser": "7.12.7",
    "@babel/plugin-transform-flow-strip-types": "7.12.1",
    "@babel/plugin-transform-runtime": "7.12.1",
    "@babel/preset-env": "7.12.7",
    "@babel/runtime": "7.12.5",
    "@babel/template": "7.12.7",
    "@babel/traverse": "7.12.8",
    "@babel/types": "7.12.7",
    "axios": "0.19.2",
    "browserify": "17.0.0",
    "mkdirp": "1.0.4",
    "node-fetch": "2.6.0",
    "prettier": "1.19.1",
    "terser": "5.5.1",
    "tmp": "0.2.1",
    "validate-npm-package-name": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.8",
    "jest": "24.9.0",
    "moment": "2.29.1",
    "standard": "14.3.4"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist/*"
    ],
    "env": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*",
      "!<rootDir>/node_modules/"
    ]
  }
}
