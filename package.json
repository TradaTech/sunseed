{
  "name": "@iceteachain/sunseed",
  "version": "0.2.0",
  "description": "Compile djs to raw js for deploying on Icetea Blockchain.",
  "main": "index.js",
  "browser": "dist/index.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "install": "npm run build",
    "test": "jest --coverage",
    "lint": "standard",
    "fix": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TradaTech/sunseed.git"
  },
  "author": "Trada Tech <iceteachain@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TradaTech/sunseed/issues"
  },
  "homepage": "https://github.com/TradaTech/sunseed#readme",
  "dependencies": {
    "@babel/core": "^7.7.4",
    "@babel/parser": "^7.7.4",
    "@babel/plugin-transform-flow-strip-types": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/runtime": "^7.7.4",
    "@babel/template": "^7.7.4",
    "@babel/traverse": "^7.7.4",
    "@babel/types": "^7.7.4",
    "axios": "^0.19.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "terser": "^4.4.0",
    "validate-npm-package-name": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "jest": "^24.9.0",
    "moment": "^2.24.0",
    "standard": "^14.3.1"
  },
  "standard": {
    "ignore": [
      "node_modules/*",
      "dist/*"
    ],
    "env": [
      "jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*",
      "!<rootDir>/node_modules/"
    ]
  }
}
